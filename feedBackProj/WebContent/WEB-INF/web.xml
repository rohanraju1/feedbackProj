 <url-pattern>/addFaculty</url-pattern>
 </servlet-mapping>
 
  <servlet>
<servlet-name>HomeAdminServlet</servlet-name>
<servlet-class>org.capG.feedBackProj.controller.HomeAdminController</servlet-class>
</servlet>

 <servlet-mapping>
 <servlet-name>HomeAdminServlet</servlet-name>
 <url-pattern>/faculSkill</url-pattern>
 </servlet-mapping>
 

 
 <servlet>
<servlet-name>ViewFacultyServlet</servlet-name>
<servlet-class>org.capG.feedBackProj.controller.ViewFacultyController</servlet-class>
</servlet>

 <servlet-mapping>
